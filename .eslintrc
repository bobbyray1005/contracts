{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "strict": 1,
    "no-return-assign": 0,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 1,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "prefer-template": 1,
    "no-useless-escape": 1,
    "no-use-before-define": 0,
    "no-return-await": 0,
    "no-else-return": 0,
    "global-require": 0,
    "no-confusing-arrow": 0,
    "max-len": ["warn", { "code": 160 }],
    "quotes": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0
  },
  "env": {
    "jest": true,
    "jasmine": true,
    "mocha": true
  },
  "plugins": [
  ]
}
